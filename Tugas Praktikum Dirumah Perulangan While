#include <iostream>
#include <string>
using namespace std;
// Tugas Praktikum Dirumah 31/03/2020
int main ( ) {
	string nama = "Wirda Hayati";
	string nim = "ETE10190122";
	cout << endl;
	cout << "Nama : " << nama;
	cout << endl;
	cout << "NIM : " << nim;
	cout << endl;
	cout << endl;
	int i = 200;
		// Menampilkan angka ganjil dan genap menggunakan while 200-1200
		cout << "Masukkan angka dari range 200-1200 : " << endl;
		cin >> i;
	while ( i <= 1200 ) {
		if ( i% 2 == 1 ) {
			cout << i << " Adalah bilangan ganjil " << endl; i++;
		} else {
			cout << i << " Adalah bilangan genap " << endl; i++;
		}
	}
}
